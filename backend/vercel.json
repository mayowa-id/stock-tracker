{
  "version": 2,
 "buildCommand": "npm install && prisma generate && npm run build",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": { "runtime": "nodejs18.x" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ]
}
